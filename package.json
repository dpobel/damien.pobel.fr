  "main": "build.js",
  "private": true,
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --recursive tests",
    "copycss": "rsync -av assets/ext/* node_modules/normalize.css/normalize.css node_modules/highlight.js/styles/github.css src/ext",
    "copyicons": "rsync -av assets/icons src/",
    "restorecache": "./bin/restore_cache.sh",
    "deploy": "./bin/deploy.sh",
    "build": "npm run copycss && npm run copyicons && ./build.js",
    "build:dev": "npm run copycss && npm run copyicons && ./build.js --dev"
  },
